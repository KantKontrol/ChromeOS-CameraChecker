{
    "name": "Camera Checker",
     "version": "1.0",
     "manifest_version": 2,
     "icons": {
       "128": "128.png",
       "96": "96.png"
     },
     "app": {
       "background": {
         "scripts": [ "background.js" ],
         "persistent": false
       }
